{
  "name": "@nebulaai/api-client",
  "version": "1.0.0",
  "description": "A powerful API client built on TanStack Query with automatic error handling and environment-aware URL resolution",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "api",
    "client",
    "tanstack-query",
    "typescript",
    "fetch"
  ],
  "author": "NebulaAI",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0",
    "@tanstack/react-query": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.11",
    "@tanstack/react-query": "^5.60.5",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3"
  }
}